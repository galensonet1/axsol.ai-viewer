{
  "name": "axsol-viewer",
  "version": "1.0.0",
  "description": "AXSOL.ai Viewer - Visualización de proyectos de construcción",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../apps/site && npm install && cd ../www && npm install && cd ../line && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:site\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:site": "cd apps/site && npm run dev",
    "dev:www": "cd apps/www && npm run dev",
    "dev:line": "cd apps/line && npm run dev",
    "build:site": "cd apps/site && npm run build",
    "build:www": "cd apps/www && npm run build",
    "build:line": "cd apps/line && npm run build",
    "start": "cd backend && npm start",
    "setup:db": "cd backend && node init-db.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "axsol",
    "viewer",
    "cesium",
    "construction",
    "3d"
  ],
  "author": "AXSOL.ai",
  "license": "PRIVATE"
}
