# Credenciales de la Base de Datos PostgreSQL
DB_USER=tu_usuario_postgres
DB_HOST=localhost
DB_DATABASE=axsol_viewer
DB_PASSWORD=tu_password_postgres
DB_PORT=5432

# Credenciales de Auth0
AUTH0_AUDIENCE=https://api.axsol-viewer.com
AUTH0_ISSUER_BASE_URL=https://tu-dominio.auth0.com/

# API Keys y configuración externa
BING_MAPS_API_KEY="TU_BING_MAPS_API_KEY"
AX_API_KEY="TU_AXSOL_API_KEY"
CESIUM_ION_ACCESS_TOKEN="TU_TOKEN_DE_CESIUM_ION"
IFC_LOCAL_DIR=/absolute/path/to/axsol-viewer/backend/python/temp

# URLs y configuración
DATABASE_URL=postgresql://tu_usuario:tu_password@localhost:5432/axsol_viewer
ADMINJS_ROOT_PATH=/admin
PORT=3001

# Configuración de polígono override (opcional)
AXSOL_POLYGON_OVERRIDE=false
AXSOL_POLYGON_OVERRIDE_PATH=

# ForestAdmin Configuration
FOREST_AUTH_SECRET=TU_FOREST_AUTH_SECRET
FOREST_ENV_SECRET=TU_FOREST_ENV_SECRET
